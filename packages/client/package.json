{
  "name": "@nilejs/client",
  "version": "0.0.1",
  "type": "module",
  "description": "Standalone, type-safe client for Nile backend framework",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "devDependencies": {
    "typescript": "^5",
    "tsup": "^8.5.1"
  }
}
